<!-- phpunit config file -->
<phpunit verbose="true">
  <testsuites>
    <testsuite name="php tests">
      <directory>phptests</directory>
    </testsuite>
  </testsuites>
  <logging>
	<log type="coverage-html" target=".coverage" charset="UTF-8"
       highlight="false" lowUpperBound="35" highLowerBound="70"/>
	</logging>
</phpunit>
